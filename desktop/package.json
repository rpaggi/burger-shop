{
  "name": "burger-shop",
  "version": "1.0.0",
  "description": "A application to manage your BurgerShop",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "d": "electron . -debug",
    "package-linux": "electron-packager . BurgerShop --platform=linux --arch=x64 --icon=ico/favicon.ico",
    "package-windows": "electron-packager . BurgerShop --platform=win32 --arch=all --icon=ico/favicon.ico"
  },
  "keywords": [
    "burger",
    "shop"
  ],
  "author": "rpaggi",
  "devDependencies": {
    "electron-packager": "^7.0.3",
    "electron-prebuilt": "^1.2.0",
    "angular"          : "^1.5.6",
    "asar"             : "^0.11.0"
  }
}
